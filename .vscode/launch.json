{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/src/generate-query-classes.ts",
            "stopOnEntry": false,
            "args": ["typings/main/ambient/core-js/index.d.ts","./src/test/test-model.ts"],
            "cwd": "${workspaceRoot}",
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/bin"
        },
         {
            "name": "Launch Test",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/src/test/test.ts",
            "stopOnEntry": false,
            "cwd": "${workspaceRoot}/src/test",
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/src/test"
        }
    ]
}