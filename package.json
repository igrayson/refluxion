{
  "name": "refluxion",
  "version": "0.0.3",
  "description": "Experimental Tool to generate Reudx and GraphQL classes from TypeScript model",
  "bin": {
    "refluxion": "bin/refluxion"
  },
  "scripts": {
    "test": "node lib/refluxion --interfaces --sequelize --graphql --client-ql --normalizr src/test/test-model.ts -o src/test/refluxion"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/refluxion.git"
  },
