    "@types/node": "6.0.38",
    "@types/normalizr": "2.0.16",
    "normalizr": "2.2.1",
    "tslint": "3.15.1"
  },
  "dependencies": {
    "commander": "2.9.0",
    "fs-extra": "0.30.0",
